<?xml version="1.0" encoding="utf-8"?>
<!-- dotnet-coverage settings for TeaLeaf.Tests -->
<Configuration>
  <CodeCoverage>
    <ModulePaths>
      <Include>
        <ModulePath>.*TeaLeaf\.dll$</ModulePath>
      </Include>
      <Exclude>
        <ModulePath>.*\.Tests\.dll$</ModulePath>
        <ModulePath>.*\.Generators\.Tests\.dll$</ModulePath>
      </Exclude>
    </ModulePaths>
  </CodeCoverage>
</Configuration>
